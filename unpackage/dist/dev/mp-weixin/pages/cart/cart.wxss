
.root {
    font-size: 12px;
    background-color: #f4f4f4;
}

/* 没登录样式 */
.notLog-box {
    width: 100vw;
    height: 100vh;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.notLog {
    text-align: center;
}
.notLog view {
    margin: 10px 0 20px 0;
}
.notLog button {
    width: 250px;
    border: 1px solid #0062cc;
    color: #0062cc;
    -webkit-border-radius: 40px;
            border-radius: 40px;
}
.notLog image {
    width: 150px;
}

